      "max_restarts": 10,
      "min_uptime": "90s",
      "exec_mode": "fork",
      "instances": 1,
      "watch": false,
      "node_args": "-r tsconfig-paths/register",
      "env": {
        "PORT": 3000,
        "NODE_ENV": "",
