        "DEFAULT_DEVICE_TYPE": "ESJHA001",
        "DB_URL": "",
        "SINGLET_POST": "false",
        "HTTPS": "true",
        "JWT_SECRET": "",
        "SUBDOMAIN": "",
        "OIDC_CLIENT_ID": "",
        "OIDC_CLIENT_SECRET": "",
        "OIDC_REDIRECT_URI": "",
        "ROOT_USER_PASSWORD": "",
        "ES_API_KEY": ""
      }
    }
  ]
}
